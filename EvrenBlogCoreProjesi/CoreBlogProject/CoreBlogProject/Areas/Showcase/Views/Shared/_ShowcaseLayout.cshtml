@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("HeadPartial")
<body class="contact-page">
   @await Html.PartialAsync("NavbarPartial")
    <main class="main">
        <div class="page-title">
            <div class="container d-lg-flex justify-content-between align-items-center">
                <h1 class="mb-2 mb-lg-0">@ViewBag.t</h1>
                <nav class="breadcrumbs">
                    <ol>
                        <li><a href="index.html" style="font-weight:bold">@ViewBag.t2</a></li>
                        <li class="current"><span style="color:red;font-size:15px">@ViewBag.t3</span></li>
                    </ol>
                </nav>
            </div>
        </div>
        <section id="contact" class="contact section">
            <div class="container" data-aos="fade-up" data-aos-delay="100">
                @RenderBody()
            </div>
        </section>
    </main>
     @await Html.PartialAsync("FooterPartial")
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
    <div id="preloader"></div>
   @await Html.PartialAsync("ScriptPartial")
</body>
@*Ajaxları kullanmam için gerekli*@
@await RenderSectionAsync("Scripts",required:false);
</html>